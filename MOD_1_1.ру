#Уровень 3
x=float(input('Введите 1е число: '))
y=float(input('Введите 2е число: '))

if x>y:
	print(x, end=' ')
	print('max = ',x)
	print(y)

else:
	print(x, end=' ')
	print('max = ',y)
	print(y)