x1=int(input('Введите координаты по горизонтали 1 ладьи: '))
y1=int(input('Введите координаты по вертикали 1 ладьи: '))
x2=int(input('Введите координаты по горизонтали 2 ладьи: '))
y2=int(input('Введите координаты по вертикали 2 ладьи: '))
if x1>8 or x1<0:
	print('Вы вышли за границы доски')
	exit()
if y1>8 or y1<0:
	print('Вы вышли за границы доски')
	exit()
if x2>8 or x2<0:
	print('Вы вышли за границы доски')
	exit()
if y2>8 or y2<0:
	print('Вы вышли за границы доски')
	exit()	
if x1==x2:
	print('YES')
elif (y1==y2):
	print('YES')
else:
	print('NO')
